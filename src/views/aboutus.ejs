    <!-- Canvas divertido Start -->
    <div class="container">
        <button id="toggleBubbles" class="easter-egg-btn">Do not Push</button>
        <canvas id="bubbleCanvas"></canvas>
    </div>

    <script>
        const canvas = document.getElementById('bubbleCanvas');
        const ctx = canvas.getContext('2d');
        const toggleButton = document.getElementById('toggleBubbles');

        canvas.width = window.innerWidth;
        canvas.height = window.innerHeight;

        let bubbles = [];
        let animationFrame;
        let isAnimating = false;

        function createBubble() {
            return {
                x: Math.random() * canvas.width,
                y: Math.random() * canvas.height,
                radius: Math.random() * 10 + 5,
                speed: Math.random() * 3 + 1,
                direction: Math.random() * Math.PI * 2
            };
        }

        for (let i = 0; i < 100; i++) {
            bubbles.push(createBubble());
        }

        function drawBubbles() {
            ctx.clearRect(0, 0, canvas.width, canvas.height);
            ctx.fillStyle = 'rgba(0, 123, 255, 0.5)';
            bubbles.forEach((bubble, index) => {
                bubble.x += Math.cos(bubble.direction) * bubble.speed;
                bubble.y += Math.sin(bubble.direction) * bubble.speed;

                if (bubble.x < 0 || bubble.x > canvas.width || bubble.y < 0 || bubble.y > canvas.height) {
                    bubbles[index] = createBubble();
                }

                ctx.beginPath();
                ctx.arc(bubble.x, bubble.y, bubble.radius, 0, Math.PI * 2);
                ctx.fill();
            });
            animationFrame = requestAnimationFrame(drawBubbles);
        }

        // Toggle the animation on button click
        toggleButton.addEventListener('click', function() {
            if (!isAnimating) {
                canvas.style.display = 'block';
                drawBubbles();
                toggleButton.textContent = "Okay... so you did push...";
            } else {
                cancelAnimationFrame(animationFrame);
                canvas.style.display = 'none';
                toggleButton.textContent = "Do not Push";
            }
            isAnimating = !isAnimating;
        });

    </script>
