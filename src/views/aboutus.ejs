    <script>
        const texts = ["CashMe", "Personal Finance"];
        const animatedTextElement = document.getElementById("animatedText");

        function animateText(text, delay) {
            let fullText = "";

            // Mostrar el texto de forma incremental
            for (let i = 0; i <= text.length; i++) {
                setTimeout(() => {
                    animatedTextElement.innerText = fullText + text.substring(0, i);
                }, delay * i);
            }

            // Mostrar el texto de forma decreciente
            const decrementDelay = delay * text.length; // Tiempo total para mostrar el texto completo
            setTimeout(() => {
                for (let i = text.length; i >= 0; i--) {
                    setTimeout(() => {
                        animatedTextElement.innerText = fullText + text.substring(0, i);
                    }, delay * (text.length - i));
                }
            }, decrementDelay);
            
            return decrementDelay + (delay * text.length); // Retorna el tiempo total que toma mostrar y ocultar el texto
        }

        function startAnimation() {
            let delay = 150; // Tiempo en milisegundos entre cada letra
            let currentTextIndex = 0;

            function animateNextText() {
                const duration = animateText(texts[currentTextIndex], delay);
                currentTextIndex = (currentTextIndex + 1) % texts.length; // Cambiar de texto circularmente

                setTimeout(animateNextText, duration + delay); // Esperar a que termine la animación del texto actual
            }

            animateNextText();
        }

        // Iniciar la animación al cargar la página
        window.onload = startAnimation;

    </script>

